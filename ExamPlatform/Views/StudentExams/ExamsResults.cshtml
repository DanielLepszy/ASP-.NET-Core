@model IList<ExamPlatform.Models.UserEmailInfoModel>
@{
    ViewData["Title"] = "Wyniki egzaminów";
}
<div id="ExamResults">
    <h2>Results</h2>
   
    <table class="table table-dark">
        <thead>
            <tr>
                <th>Firstname</th>
                <th>Lastname</th>
                <th>Email</th>
                <th>Exam Date</th>
                <th>Course</th>
                <th>Grade</th>
                <th>-</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var x in Model)
            {
                <tr>
                    <td>@x.Name</td>
                    <td>@x.Surname</td>
                    <td>@x.Email</td>
                    <td>@x.ExamDate</td>
                    <td>@x.Course</td>
                    <td>@x.Grade.0</td>
                </tr>
            }
        </tbody>
    </table>
</div>